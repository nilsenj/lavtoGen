{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "namshi/jose": "^6.0",
        "doctrine/dbal": "^2.5",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "barryvdh/laravel-ide-helper": "^2.1",
        "pingpong/generators": "^2.2",
        "league/fractal": "^0.13.0",
        "robclancy/presenter": "^1.3",
        "nilsenj/toastr-5.1-laravel": "dev-master",
        "laravelcollective/html": "~5.1",
        "torann/hashids": "^2.0",
        "juggl/unique-hashids": "^1.1",
        "codezero/mailer": "^1.0",
        "intervention/imagecache": "~2.3.3",
        "cviebrock/image-validator": "2.0.0",
        "spatie/laravel-medialibrary": "^3.11",
        "a6digital/laravel-default-profile-image": "^1.0",
        "landish/pagination": "dev-master",
        "phpseclib/phpseclib": "^2.0",
        "barryvdh/laravel-debugbar": "^2.2",
        "league/csv": "^8.0",
        "arcanedev/log-viewer": "^3.9",
        "symfony/psr-http-message-bridge": "^0.2.0",
        "barryvdh/laravel-cors": "^0.8.0",
        "webpatser/laravel-countries": "^1.4"

    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database",
            "core/Models",
            "core/Presenters",
            "core/Repositories",
            "core/Traits"
        ],
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/",
            "RepositoryLab\\Repository\\": "core/RepositoryLab/",
            "Core\\Modular\\": "core/modular/",
            "Core\\Models\\": "core/Models/",
            "Core\\Presenters\\": "core/Presenters/",
            "Core\\Repositories\\": "core/Repositories/",
            "Core\\Traits\\": "core/Traits/",
            "Modules\\": "modules/",
            "Core\\Contracts\\": "core/Contracts/",
            "Core\\MailComposers\\": "core/MailComposers/",
            "Core\\Components\\": "core/Components/",
            "Core\\ServiceIntegrations\\": "core/ServiceIntegrations/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
